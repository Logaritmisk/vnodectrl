#!/bin/bash

# api_version=1


function umount_info() {
    echo "Umount guest system"
}

function umount_exec() {
    mount | egrep "^$(echo $GUEST_ADDR | sed 's/\./\\\./g'):/srv on.*$" > /dev/null

    if [ ! $? -eq 0 ]; then
        echo "There is no mount to unmount."
        return 1
    fi


    sudo umount $SRV_FOLDER
}