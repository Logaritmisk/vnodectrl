#!/bin/bash

# api_version=1


umount_info() {
    echo "Umount guest system"
}

umount_exec() {
    mount | egrep "^$(echo $(vnode_identity_get 'guest_addr') | sed 's/\./\\\./g'):/srv on.*$" > /dev/null

    if [ ! $? -eq 0 ]; then
        echo "There is no mount to unmount."
        return 1
    fi


    sudo umount $(vnode_identity_get 'srv_folder')
}