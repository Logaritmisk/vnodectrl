#!/bin/bash

# api_version=1


apache2_info() {
    echo "Control apache2 on guest system"
}

apache2_completion() {
    echo "start stop reload restart"
}

apache2_exec() {
    ssh -qt -oStrictHostKeyChecking=no $GUEST_USER@$GUEST_ADDR "echo $GUEST_PASS | sudo -p '' -S /etc/init.d/apache2 $1" | tail -n +1
}
